  "description": "Push GitHub variables and secrets using Octokit for repo-level and organization-level",
  "files": [
    "dist"
  ],
  "bin": {
    "gh-push-vars": "dist/index.js"
  },
  "keywords": [
    "github",
    "cli",
    "secrets",
    "variables",
    "octokit"
  ],
  "author": "Anggit M Ginanjar",
  "license": "MIT",
  "module": "index.ts",
  "type": "module",
